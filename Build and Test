pipeline{
  agent any
  environment{
  branch='master'
  source=''-------------server port='8080'
  }
  stages{
          stage('checkout git'){
             steps{
                 git url:'---------------'
                 sh 'git clean';             }
                  }              
        }
          stage('build'){
            steps{
                  sh'mvn clean package'
                 }
                        }
          stage('test'){
            steps{  
                  parallel 'test': {
                  sh "${mvnHome}/bin/mvn test;
    
  
  }  
}


  
