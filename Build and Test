pipeline{
  agent any
  environment{
  branch='master'
  }
  stages{
          stage('checkout git'){
            steps{
                 git url:'---------------'
                 sh 'git clean'; 
                                }
                  }             
          stage('build'){
            steps{
                  sh'mvn clean package'
                        }
                 }
            
          stage('UAT Test') {
            steps {
                    sh "UAT tests-${ENVIRONMENT}.sh"
                    }
                
                }            
          stage('SIT Tests') {
            steps {
                    sh "SIT tests-${ENVIRONMENT}.sh"
                   }
                   
            }
        }
    }
